{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true,
        "jasmine": true,
        "codeceptjs/codeceptjs": true
    },
    "extends": ["airbnb-base","plugin:codeceptjs/recommended"],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": 0,
        "no-underscore-dangle": 0,
        "no-trailing-spaces": 0,
        "indent": 0,
        "no-console": 0,
        "quotes": 0,
        "eol-last": 0,
        "object-curly-newline": 0,
        "object-curly-spacing": 0,
        "max-len": 0,
        "lines-between-class-members": 0,
        "no-whitespace-before-property": 0,
        "keyword-spacing": 0,
        "no-restricted-globals": 0,
        "no-else-return": 0,
        "no-multiple-empty-lines": 0,
        "import/no-useless-path-segments": 0,
        "arrow-parens": 0,
        "no-undef": 0,
        "no-prototype-builtins": 0,
        "consistent-return": 0,
        "import/prefer-default-export": 0,
        "eqeqeq": 0,
        "no-return-assign": 0,
        "no-unused-vars": 0,
        "no-empty-function": 0,
        "class-methods-use-this": 0,
        "no-shadow": 0,
        "no-new": 0,
        "import/no-extraneous-dependencies": 0,
        "semi": 0,
        "space-before-function-paren": 0,
        "func-names": 0,
        "prefer-destructuring": 0,
        "import/order": 0,
        "key-spacing": 0,
        "arrow-spacing": 0,
        "semi-spacing": 0,
        "space-infix-ops": 0,
        "block-spacing": 0,
        "spaced-comment": 0,
        "no-var": 0,
        "comma-spacing": 0,
        "no-void": 0,
        "vars-on-top": 0,
        "no-sequences": 0,
        "no-multi-assign": 0,
        "one-var-declaration-per-line": 0,
        "no-unused-expressions": 0,
        "block-scoped-var": 0,
        "no-plusplus": 0,
        "no-restricted-syntax": 0,
        "guard-for-in": 0,
        "space-before-blocks": 0,
        "yoda": 0,
        "one-var":0,
        "no-param-reassign": 0,
        "no-use-before-define": 0,
        "no-redeclare": 0,
        "no-loop-func": 0,
        "no-bitwise": 0,
        "prefer-arrow-callback": 0,
        "switch-colon-spacing": 0,
        "padded-blocks": 0,
        "no-fallthrough": 0,
        "default-case": 0,
        "no-cond-assign": 0,
        "object-shorthand": 0,
        "prefer-rest-params": 0,
        "no-nested-ternary": 0,
        "no-proto": 0,
        "prefer-template": 0,
        "no-mixed-operators": 0,
        "no-continue": 0,
        "new-cap": 0,
        "no-func-assign": 0,
        "strict": 0,
        "lines-around-directive": 0,
        "new-parens": 0,
        "no-useless-escape": 0,
        "wrap-iife": 0,
        "no-new-func": 0,
        "newline-per-chained-call": 0,
        "prefer-spread": 0,
        "space-unary-ops": 0,
        "no-floating-decimal": 0,
        "no-return-await": 0,
        "generator-star-spacing": 0,
        "prefer-object-spread": 0,
        "no-await-in-loop": 0,
        "no-empty": 0,
        "no-confusing-arrow": 0,
        "no-promise-executor-return": 0,
        "max-classes-per-file": 0,
        "import/no-amd": 0,
        "prefer-const": 0
    },
    "plugins": ["jasmine", "codeceptjs"]
}
